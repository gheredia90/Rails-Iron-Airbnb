<div class="row">
    <% @flats.each do |f|%>

          <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
              <img src=<%= f.pic_url %> alt="...">
              <div class="caption">
                <h3><%= f.title %></h3>
                <p><%= f.description %></p>
                <p><a href="#" class="btn btn-primary" role="button">See more</a>
                <a href="#" class="btn btn-primary" role="button"><%= f.owner %></a>
                <%= button_to "Delete", flat_path(f), method: :delete,
                  data: { confirm: 'Are you sure?' } %>               
                </p>
              </div>
            </div>
          </div>

    <% end %>

</div>